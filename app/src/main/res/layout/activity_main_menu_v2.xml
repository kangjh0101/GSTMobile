<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#90D400"
    tools:context=".ui.main.MainMenuV2Activity">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/mainMenuActivity2ScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#90D400">

        <!--<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#90D400"
            tools:context=".ui.main.MainMenuV2Activity">-->

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="411dp"
                android:layout_height="99dp"
                android:scaleX="1.2"
                android:scaleY="1.2"
                android:src="@drawable/grandslam_2nd_note_2440x1080px_r144_g212_b0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/wanted_ad_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/wanted_ad_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView1" />

            <ImageButton
                android:id="@+id/post_ad_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/post_wanted_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/wanted_ad_btn" />

            <ImageButton
                android:id="@+id/my_ad_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/my_wanted_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/post_ad_btn" />

            <ImageButton
                android:id="@+id/msg_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/check_message_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/my_ad_btn" />

            <ImageButton
                android:id="@+id/new_msg_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/new_messages_note_2440x1080px_r180_g0_b0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/my_ad_btn" />

            <ImageButton
                android:id="@+id/lesson_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/lesson_coach_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/msg_btn" />

            <ImageButton
                android:id="@+id/club_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/club_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lesson_btn" />

            <ImageButton
                android:id="@+id/hand_over_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/hand_over_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/club_btn" />

            <ImageButton
                android:id="@+id/report_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/bug_report_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/hand_over_btn" />

            <ImageButton
                android:id="@+id/drop_btn"
                android:layout_width="328dp"
                android:layout_height="44dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:adjustViewBounds="true"
                android:background="#90D400"
                android:scaleType="fitCenter"
                android:scaleX="1"
                android:scaleY="1"
                android:src="@drawable/drop_user_note_2440x1080px_r244_g189_b39"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_btn" />

            <com.google.android.gms.ads.AdView
                android:id="@+id/MainMenuPageAdView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="2dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="2dp"
                app:adSize="BANNER"
                app:adUnitId="ca-app-pub-2436476461762188/7077830504"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/drop_btn" />

        <!--</androidx.constraintlayout.widget.ConstraintLayout>-->
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>